
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>WICC MIS</title>
    <script src="~/scripts/bootstrap.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/scripts/jquery-3.1.1.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>
    <script>

        $(function () {
            Highcharts.chart('yearlysales', {
                chart: {
                    type: 'column'
                },
                title: {
                    text: 'Yearly Sales'
                },
                xAxis: {
                    categories: ['2015-16', '2016-17']
                },
                credits: {
                    enabled: false
                },
                series: [{
                    name: 'Local',
                    data: [30822, 40822]
                }, {
                    name: 'Export',
                    data: [208240, 408240]
                }]
            });
        });

        //Product_Wise_Sales
        $(function () {
            Highcharts.chart('Product_Wise_Sales', {
                chart: {
                    type: 'column'
                },
                title: {
                    text: 'Product Wise Sales'
                },
                xAxis: {
                    categories: ['2015-16', '2016-17']
                },
                credits: {
                    enabled: false
                },
                series: [{
                    name: 'Dry Roasted Almond 2-4 mm',
                    data: [30822, 40822]
                }, {
                    name: 'Dry Roasted Cashew 2-4 mm',
                    data: [208240, 408240]
                }]
            });
        });



    </script>
   
</head>
<body>
    <div>
        
    </div>


    <div class="row">
        <div class="col-md-4"><div id="yearlysales" style="min-width: 155px; height: 400px; margin: 0 auto"></div></div>
        <div class="col-md-8"><div id="Product_Wise_Sales" style="min-width: 155px; height: 400px; margin: 0 auto"></div></div>
    </div>
    
  
</body>
</html>
